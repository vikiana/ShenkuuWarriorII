package virtualworlds.helper.Fu.BaseEvents{	import flash.events.Event;		public class ConnectorEvent extends Event	{		public static const ALLITEMSLOADED:String = "allItemsLoaded";		public static const ITEMLOADED:String = "itemLoaded";		public static const ITEMPROGRESS:String = "itemProgress";				public var loadedItem:*		public var amountLoaded:uint		public var amountTotal:uint		public var fileLength:int; 		public var numberOfFiles:int; 				public function ConnectorEvent(aEventType:String, aItem:* = null, loaded:uint = 0, total:uint = 0, fLength:int = 0, numOfFiles:int = 0)		{			loadedItem = aItem;			amountLoaded = loaded;			amountTotal = total;			fileLength = fLength; 			numberOfFiles = numOfFiles;						super(aEventType)		}	}}